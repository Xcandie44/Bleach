<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sdadasads"
    tools:context=".Fourth">

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="35dp"
        android:layout_marginTop="127dp"
        android:text="Есть ли у тебя \nкакие-либо комплексы?" />

    <Button
        android:id="@+id/yes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:onClick="onYesClick"
        android:layout_alignTop="@+id/no"
        android:layout_marginStart="31dp"
        android:text="Да" />

    <Button
        android:id="@+id/no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button8"
        android:onClick="onNoClick"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="148dp"
        android:text="Нет" />

    <Button
        android:id="@+id/maybe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onMaybeClick"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="39dp"
        android:text="Возможно" />
</RelativeLayout>